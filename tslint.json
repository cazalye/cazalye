{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {
        "semicolon": true
    },
    "rules": {
        "semicolon": true,
        "object-literal-shorthand": false,
        "no-conditional-assignment": false,
        "jsx-no-multiline-js": false
    },
    "rulesDirectory": []
}